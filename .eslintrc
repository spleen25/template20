{
  "globals": {
    "Waypoint": true
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "extends": [
    "airbnb",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "prettier/standard"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this:": 0,
    "jsx-a11y/no-static-element-interactions": ["error", {"handlers": [] }],
    "id-length": [1, { "min": 1 }],
    "arrow-parens": ["error", "as-needed"],
    "max-len": [2, 200, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "object-curly-newline": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": [2, { "forbid": ["any"] }],
    "jsx-a11y/label-has-for": [ 2, {
      "components": [ "Label" ],
      "required": {
        "every": [ "nesting", "id" ]
      },
      "allowChildren": false
    }],
    "prettier/prettier": "error"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
